{
    "name": "sparkinzy/dcat-websocket",
    "alias": "",
    "description": "为Dcat Admin 一键集成websocket功能",
    "type": "library",
    "keywords": ["dcat-admin", "extension","websocket"],
    "homepage": "https://github.com/sparkinzy/dcat-websocket",
    "license": "MIT",
    "authors": [
        {
            "name": "sparkinzy",
            "email": "sparkinzy@163.com"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "dcat/laravel-admin": "~2.0",
        "pusher/pusher-php-server": "~4.0",
        "beyondcode/laravel-websockets": "^1.13"
    },
    "autoload": {
        "psr-4": {
            "Sparkinzy\\DcatWebsocket\\": "src/"
        }
    },
    "extra": {
        "dcat-admin": "Sparkinzy\\DcatWebsocket\\DcatWebsocketServiceProvider",
        "laravel": {
            "providers": [
                "Sparkinzy\\DcatWebsocket\\DcatWebsocketServiceProvider"
            ]
        }
    }
}
